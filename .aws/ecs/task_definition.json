{
    "containerDefinitions": [
      {
        "memory": 512,
        "essential": true,
        "entryPoint": [
          "python3"
        ],
        "name": "poc-python-ecs-task-definition",
        "readonlyRootFilesystem": true,
        "image": "175965032279.dkr.ecr.us-east-1.amazonaws.com/poc-python-ecs:latest",
        "command": [
          "./upload.py"
        ],
        "cpu": 256,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-region": "us-east-1",
                "awslogs-group": "poc-python-ecs-task-definition-logs"
            }
        }
      }
    ],
    "family": "poc-python-ecs-family",
    "requiresCompatibilities": ["FARGATE"],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::175965032279:role/poc-python-ecs-role"
    
  }